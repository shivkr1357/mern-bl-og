   "dependencies": {
      "bcrypt": "^5.0.0",
      "body-parser": "^1.19.0",
      "bootstrap": "^4.5.2",
      "concurrently": "^5.3.0",
      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-validator": "^6.6.1",
      "jsonwebtoken": "^8.5.1",
      "mdbreact": "^4.27.0",
      "mongoose": "^5.10.5",
      "nodemon": "^2.0.4",
      "react-share": "^4.3.0"
   },
   "devDependencies": {
      "morgan": "^1.10.0"
   }
